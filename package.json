{
  "name": "vonage-video-react-app",
  "version": "1.3.0",
  "description": "React reference application for the Vonage Video web client SDK.",
  "private": true,
  "engines": {
    "node": ">=22.x",
    "npm": "9.x"
  },
  "scripts": {
    "quality-check": "nx run-many -t ts-check,lint --parallel --maxParallel=4",
    "build": "nx run frontend:build",
    "deploy-vcr": "vcr deploy",
    "dev:frontend": "nx serve frontend --host",
    "dev:backend": "nx serve backend",
    "dev": "concurrently 'nx serve frontend --host' 'nx serve backend'",
    "docs": "nx run frontend:docs",
    "docs:watch": "nx run frontend:docs:watch",
    "lint": "nx run-many -t lint",
    "lint:filenames": "./scripts/lintFileNames.sh",
    "lint:fix": "prettier --log-level warn --write \"**/*.{ts,tsx,js,json,md,yml,yaml}\" && nx run-many -t lint -- --fix",
    "run-server": "nx run backend:start",
    "start": "nx run frontend:build && nx run backend:start",
    "start:backend": "nx serve backend",
    "debug:backend": "nx run backend:debug",
    "start:frontend": "nx serve frontend --host",
    "test": "nx run-many -t test --projects=backend,frontend --parallel --maxParallel=3",
    "test:backend": "nx test backend",
    "test:backend:watch": "nx test backend --watch",
    "test:frontend": " nx test frontend",
    "test:frontend:": "nx test frontend -- --reporter=verbose --coverage=false --bail=1 --run --include",
    "test:frontend:debug": "vitest --disableConsoleIntercept=true --silent=false --no-file-parallelism --inspect-brk --root frontend --config vite.config.ts --no-coverage",
    "test:frontend:watch": "nx test frontend --watch",
    "test:integration": "nx run integration-tests:test",
    "ts-check": "nx run backend:ts-check && nx run frontend:ts-check",
    "test:integrationUpdateScreenshots": "nx run integration-tests:updateScreenshots",
    "ts-check:backend": "nx run backend:ts-check",
    "ts-check:frontend": "nx run frontend:ts-check"
  },
  "scriptsInfo": {
    "dev": "Run both frontend and backend in development mode with hot reload",
    "debug:backend": "Run backend in debug mode to attach a debugger (e.g., chrome://inspect)",
    "test:frontend": "Run frontend tests via Nx",
    "test:frontend:": "Example: 'yarn test:frontend: <name/path>' to run specific tests without coverage",
    "test:frontend:debug": "Debug frontend tests with Vitest/Node inspector"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-video-react-app.git"
