{
  "name": "vonage-video-react-app",
  "version": "1.3.0",
  "description": "React reference application for the Vonage Video web client SDK.",
  "private": true,
  "engines": {
    "node": ">=22.x",
    "npm": "9.x"
  },
  "scripts": {
    "quality-check": "nx run-many -t ts-check,lint --parallel",
    "build": "nx run frontend:build",
    "deploy-vcr": "vcr deploy",
    "dev:frontend": "nx serve frontend --host",
    "dev:backend": "nx serve backend",
    "dev": "concurrently 'nx serve frontend --host' 'nx serve backend'",
    "docs": "nx run frontend:docs",
    "docs:watch": "nx run frontend:docs:watch",
    "lint": "ESLINT_USE_FLAT_CONFIG=false yarn eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:filenames": "./scripts/lintFileNames.sh",
    "lint:fix": "prettier --log-level warn --write \"**/*.{ts,tsx,js,json,md,yml,yaml}\" && yarn lint --fix",
    "prepare": "husky install",
    "run-server": "nx run backend:start",
    "start": "nx run frontend:build && nx run backend:start",
    "start:backend": "nx serve backend",
    "debug:backend": "nx run backend:debug",
    "start:frontend": "nx serve frontend --host",
    "test": "nx run-many -t test --projects=backend,frontend --parallel",
    "test:backend": "nx test backend",
    "test:backend:watch": "nx test backend --watch",
    "test:frontend": "nx test frontend",
    "test:frontend:": "nx test frontend -- --bail 1 --no-coverage",
    "test:frontend:debug": "vitest --disableConsoleIntercept=true --silent=false --no-file-parallelism --inspect-brk --root frontend --config vite.config.ts --no-coverage",
    "test:frontend:watch": "nx test frontend --watch",
    "test:integration": "nx run integration-tests:test",
    "ts-check": "nx run backend:ts-check && nx run frontend:ts-check",
    "test:integrationUpdateScreenshots": "nx run integration-tests:updateScreenshots",
    "ts-check:backend": "nx run backend:ts-check",
    "ts-check:frontend": "nx run frontend:ts-check"
  },
  "scriptsInfo": {
    "dev": "Run both frontend and backend in development mode with hot reload",
    "debug:backend": "Run backend in debug mode to attach a debugger (e.g., chrome://inspect)",
    "test:frontend": "Run frontend tests via Nx",
    "test:frontend:": "Example: 'yarn test:frontend: <name/path>' to run specific tests without coverage",
    "test:frontend:debug": "Debug frontend tests with Vitest/Node inspector"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-video-react-app.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-video-react-app/issues"
  },
  "homepage": "https://github.com/Vonage/vonage-video-react-app#readme",
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.14.4",
    "@nx/eslint": "22.0.1",
    "@nx/jest": "22.0.1",
    "@nx/js": "22.0.1",
    "@nx/playwright": "22.0.1",
    "@nx/react": "^22.0.1",
    "@nx/vite": "^22.0.1",
    "@nx/web": "22.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "ajv": "^8.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.2.2",
    "nx": "^22.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "tslib": "^2.3.0",
    "typescript": "^5.8.3",
    "verdaccio": "^6.0.5",
    "vite": "^5.4.19",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^1.3.1"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "path-to-regexp": "^0.1.12",
    "express": "^4.21.2",
    "axios": "^1.12.0",
    "cross-spawn": "^7.0.5",
    "rollup": "^4.22.4",
    "vite": "^5.4.19",
    "nanoid": "^3.3.8",
    "micromatch": "^4.0.8",
    "@babel/helpers": "^7.27.0",
    "@babel/runtime": "^7.26.10",
    "brace-expansion": "2.0.2",
    "form-data": "^4.0.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
